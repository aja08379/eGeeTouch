<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="100dp"/>
            <gradient android:startColor="#c9e1fc" android:endColor="#c9e1fc" android:angle="45" android:type="linear" android:centerX="35%" android:centerColor="#c9e1fc"/>
            <padding android:left="0dp" android:top="0dp" android:right="0dp" android:bottom="0dp"/>
            <size android:height="60dp" android:width="60dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="100dp"/>
            <gradient android:startColor="@color/cyanblue" android:endColor="@color/cyanblue" android:angle="45" android:type="linear" android:centerX="35%" android:centerColor="@color/cyanblue"/>
            <padding android:left="0dp" android:top="0dp" android:right="0dp" android:bottom="0dp"/>
            <size android:height="60dp" android:width="60dp"/>
        </shape>
    </item>
</selector>
